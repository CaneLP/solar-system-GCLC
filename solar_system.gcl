dim 800 600
background 0 0 0

procedure rev {ang ret}
{
	expression ret { ang - floor(ang/360)*360 }
}


procedure atan2 { y x ret }
{
  number PI 3.14159265359
  if_then_else { x > 0 }
  {
    expression ret { atan(y/x) }
  } { }
  if_then_else { x < 0 && y >= 0 }
  {
    expression ret { atan(y/x) + PI }
  } { }
  if_then_else { x < 0 && y < 0 }
  {
    expression ret { atan(y/x) - PI }
  } { }
  if_then_else { x == 0 && y > 0 }
  {
    expression ret { PI/2 }
  } { }
  if_then_else { x == 0 && y < 0 }
  {
    expression ret { -PI/2 }
  } { }
  if_then_else { x == 0 && y == 0 }
  {
    expression ret { 99999999999 }
  } { }
}


