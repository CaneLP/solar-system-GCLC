% sun
expression sun_w { 282.9404 + 0.0000470935 * d }
number sun_a 1
expression sun_e { 0.016709 - 0.000000001151 *d }
expression sun_M { 356.0470 + 0.9856002585 * d }
expression oblecl { 23.4393 - 0.0000003563*d }
% expression L { sun_w + sun_M }  % mean longitude L
call rev { sun_M sun_M_normalized }
expression L { sun_w + sun_M_normalized }  % mean longitude L
call rev { L L_normalized }

% expression sun_E { sun_M_normalized + (180/PI) * sun_e * sin(sun_M_normalized) * (1 + sun_e * cos(sun_M_normalized)) }
expression sun_E { sun_M_normalized + 180/PI * sun_e * sin(sun_M_normalized*PI/180) * (1 + sun_e * cos(sun_M_normalized*PI/180)) }

expression sun_X { cos(sun_E*PI/180) -  sun_e }
expression sun_Y { sin(sun_E*PI/180) * sqrt(1 - sun_e*sun_e) }

expression sun_r { sqrt(sun_X*sun_X + sun_Y*sun_Y) }
call atan2 { sun_Y sun_X sun_v }
expression sun_v_deg { 180/PI * sun_v }

expression sun_long { sun_v_deg + sun_w }
call rev { sun_long sun_long_normalized}

% erc - ecliptic rectangular coordinates
expression sun_x_erc { sun_r * cos(sun_long_normalized * PI/180) }
expression sun_y_erc { sun_r * sin(sun_long_normalized * PI/180) }
number sun_z 0

% incorporate eccentricity
expression sun_x_final { sun_x_erc }
expression sun_y_final { sun_y_erc * cos(oblecl * PI/180) }
expression sun_z_final { sun_y_erc * sin(oblecl * PI/180) }

expression sun_x_viz { SIZE_FACTOR * sun_x_final + earthx }
expression sun_y_viz { SIZE_FACTOR * sun_y_erc * cos(oblecl * PI/180) + earthy }
expression sun_z_viz { SIZE_FACTOR * sun_y_erc * sin(oblecl * PI/180) + 0}


